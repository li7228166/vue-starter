{
  "name": "Test",
  "version": "1.0.0",
  "description": "react redux router",
  "main": "server.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "yarn run clean && cross-env NODE_ENV=development node scripts/server.js",
    "start:with_happypack": "yarn run clean && cross-env NODE_ENV=development USE_HAPPYPACK=true node scripts/server.js",
    "start:dll": "yarn run dll && yarn start",
    "start:proxy": "yarn run clean && cross-env NODE_ENV=development PROXY=true node scripts/server.js",
    "build": "yarn run clean && cross-env NODE_ENV=production webpack --config scripts/webpack.config.prod.js  --progress --hide-modules",
    "release": "yarn run build && node scripts/server.js",
    "release:proxy": "cross-env PROXY=true yarn run build && node scripts/server.js",
    "deploy": "yarn run build && gulp upload",
    "dll": "rimraf dll && webpack --config scripts/webpack.config.dll.js  --progress --hide-modules",
    "postinstall": "yarn run dll",
    "profile": "webpack --config scripts/webpack.config.dev.js --profile --json > stats.json"
  },
  "author": "lxp",
  "license": "ISC",
  "dependencies": {
    "animate.css": "^3.5.2",
    "vue": "^2.5.13"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.1.2",
    "autoprefixer": "^7.2.3",
    "babel-core": "^6.8.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "body-parser": "^1.15.2",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.7",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "gulp": "^3.9.1",
    "gulp-ssh": "^0.6.0",
    "happypack": "^4.0.0",
    "html-webpack-plugin": "^2.16.1",
    "less": "^2.6.1",
    "less-loader": "^4.0.5",
    "open": "0.0.5",
    "postcss-loader": "^2.0.9",
    "precss": "^2.0.0",
    "query-string": "^5.0.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.7.0",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.2",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-merge": "^4.1.1"
  }
}
